#!/usr/bin/python3

class Cachorro:
    def __init__(self, cor = 'branco'):
        self._cor = cor
        
    def latir(self):
        print(self._numero,"Uow, Uow...")
        
    def balancar(self):
        print(self._numero,"Balancou o rabo")
        
    def get_cor(self):
        return self._cor
        
def main():
    raffy = Cachorro(1)
    raffy._cor = "preto"
    print(raffy._cor)
    
    
if __name__ == "__main__" : main()