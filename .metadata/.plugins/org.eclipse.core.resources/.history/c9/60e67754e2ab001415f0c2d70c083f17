#!/usr/bin/python3

class Cachorro:
    def __init__(self, numero):
        self._numero = numero
        print('Criando cachorro!')
        
    def latir(self):
        print(self._numero,"Uow, Uow...")
        
    def balancar(self):
        print(self._numero,"Balancou o rabo")
        
def main():
    raffy = Cachorro()
    raffy.latir()
    raffy.balancar()
    
if __name__ == "__main__" : main()